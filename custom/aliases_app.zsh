#http://www.sublimetext.com/docs/2/osx_command_line.html
#ln -s "/Applications/Sublime Text.app/Contents/SharedSupport/bin/subl" /usr/local/bin/subl

alias subl='/opt/homebrew/bin/subl -a'
alias sublnew='/opt/homebrew/bin/subl -n'
alias sublhelp='/opt/homebrew/bin/subl --help'
alias python=/usr/local/bin/python3
alias pip=/usr/local/bin/pip3
alias ghpr='gh pr create --base master --fill --web'
alias browser='open -a "Google Chrome"'

export EDITOR='/opt/homebrew/bin/subl -w'